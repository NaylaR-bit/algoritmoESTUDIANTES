Algoritmo cantidadAlumnos
	Definir  TOTAL_ESTUDIANTES Como Entero;
	TOTAL_ESTUDIANTES = 15;
	
	Definir estudiantes_faltantes Como Entero;
	estudiantes_faltantes = 0;
	
	Escribir "Cantidad de Alumnos TOTAL";
	leer TOTAL_ESTUDIANTES;
	
	Escribir "Cantidad Alumnos Faltantes HOY"
	leer estudiantes_faltantes;
	
	leer (TOTAL_ESTUDIANTES - estudiantes_faltantes)
      
	
FinAlgoritmo
